start hi_there;

func hi_there() -> int {
    int x = 5;
    println("this statement is \n\t\t", x > 10 or x == 6);

    loop i: 1 to 6 times {
        loop i times {
            print("*");
        }
        println();
    }

    return 0;
}

func bismach(int num, string str) -> int {
    int x = 1;
    int y = 2;
    println("x = ", x, '\t', "y = ", y);
    swap x, y;
    println("x = ", x, '\t', "y = ", y);

    return 10;
}

